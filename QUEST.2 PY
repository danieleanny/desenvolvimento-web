num1  =  int ( input ( 'Digite o primeiro número inteiro: ' ))
num2  =  int ( input ( 'Digite ou segundo número inteiro: ' ))
num3  =  int ( input ( 'Dígito ou terceiro número inteiro: ' ))
if (( num1  <  num2 ) e ( num1  <  num3 )):
    print ( 'O menor número é:' , num1 )
if (( num2  <  num1 ) e ( num2  <  num3 )):
    print ( 'O menor número é:' , num2 )
if (( num3  <  num2 ) e ( num3  <  num1 )):
    print ( 'O menor numero é:' , num3 )